{
    "name": "uf1",
    "productName": "Ultimate-F1Viewer-With-F1MV",
    "version": "1.4.8",
    "description": "A integration with multiple windows to enchance your F1TV experience using the MultiViewer for F1 app.",
    "main": "./src/index.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "watch_weather": "webpack --config src/weather/webpack.common.js --watch",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "postinstall": "electron-builder install-app-deps"
    },
    "keywords": [],
    "author": "MR. AJEKO",
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "UltimateF1Viewer"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@nivo/bullet": "^0.80.0",
        "@nivo/line": "^0.80.0",
        "adm-zip": "^0.5.9",
        "discord-rpc": "^4.0.1",
        "electron-fetch": "^1.9.1",
        "electron-reload": "^2.0.0-alpha.1",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^8.1.0",
        "fs-extra": "^8.1.0",
        "govee-lan-control": "^2.1.0",
        "npm_f1mv_api": "^1.4.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "request": "^2.88.0",
        "requirejs": "^2.3.6"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@electron-forge/cli": "^6.0.0-beta.66",
        "@electron-forge/maker-deb": "^6.0.0-beta.66",
        "@electron-forge/maker-rpm": "^6.0.0-beta.66",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
        "@electron-forge/maker-zip": "^6.0.0-beta.66",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.3",
        "electron": "^22.1.0",
        "electron-builder": "^24.0.0-alpha.9",
        "electron-packager": "^16.0.0",
        "electron-prebuilt-compile": "^1.3.2",
        "style-loader": "^3.3.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    }
}
